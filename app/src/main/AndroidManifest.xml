<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lasakpedia.carwash">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.Fullscreen.NoActionBar">
        <activity android:name=".SplashAct">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeAct"
            android:label="@string/title_activity_home"
            android:theme="@style/AppTheme.Fullscreen.NoActionBar"></activity>
        <activity android:name=".GetStartedAct" />
        <activity android:name=".SignInAct" />
        <activity android:name=".RegisterOneAct" />
        <activity android:name=".RegisterTwoAct" />
        <activity android:name=".RegisterSuccessAct" />
    </application>

</manifest>